{
    "name": "jessegall/proxy",
    "autoload": {
        "psr-4": {
            "JesseGall\\Proxy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "tests/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Jesse Gall"
        }
    ],
    "scripts": {
        "test": "@php -dxdebug.mode=coverage ./vendor/bin/phpunit --testdox --coverage-html=report --colors=auto",
        "test-filter": "@test --filter="
    },
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "symfony/var-dumper": "^6.1",
        "spatie/ray": "^1.36",
        "jessegall/invader": "^1.0"
    },
    "license": "MIT"
}
